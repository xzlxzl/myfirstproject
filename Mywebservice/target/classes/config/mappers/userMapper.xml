<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.swd.webservice.mapper.UserMapper" >
<select id="selectById" parameterType="java.lang.Integer" resultType="com.swd.webservice.vo.User">
   select user_id as userId,user_name as userName,user_no as userNo from T_user where user_id=#{userId}
</select>

</mapper>